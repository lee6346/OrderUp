{
  "name": "orderup",
  "version": "1.0.0",
  "description": "connect cooks and hungry couch potatos",
  "main": "/app/server/server.js",
  "engines": {
    "node": "8.9.3",
    "npm": "5.8.0"
  },
  "scripts": {
    "test": "mocha \"./{,!(node_modules)/**/}*.test.js\"",
    "test-watch": "nodemon --exec \"npm test\"",
    "debug": "node --nolazy --inspect-brk=9299 server.js",
    "start": "node ./app/server/server.js",
    "server": "nodemon ./app/server/server.js",
    "client": "npm run start --prefix app/client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "James Lee",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "prettier": "1.11.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.5",
    "mongoose": "^5.0.12",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "request": "^2.85.0",
    "winston": "^2.4.1"
  }
}
